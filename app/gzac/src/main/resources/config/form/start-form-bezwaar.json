{
    "display": "form",
    "components": [
        {
            "key": "firstName",
            "type": "textfield",
            "input": true,
            "label": "First name",
            "tableView": true,
            "attributes": {
                "data-testid": "bezwaar.start-firstName"
            },
            "applyMaskOn": "change",
            "defaultValue": "Peter"
        },
        {
            "key": "lastName",
            "type": "textfield",
            "input": true,
            "label": "Last name",
            "tableView": true,
            "attributes": {
                "data-testid": "bezwaar.start-lastName"
            },
            "applyMaskOn": "change",
            "defaultValue": "voor Klaver"
        },
        {
            "key": "submit",
            "type": "button",
            "input": true,
            "label": "Submit",
            "tableView": false,
            "disableOnInvalid": true
        }
    ]
}